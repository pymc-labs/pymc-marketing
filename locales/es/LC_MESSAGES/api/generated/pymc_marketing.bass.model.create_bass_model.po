msgstr ""

#: 8e3f5eb2dcce4d89810c42f62165307b of
#: pymc_marketing.bass.model.create_bass_model:24
msgid "**observed** : :obj:`pt.TensorLike` | :data:`python:None`"
msgstr ""

#: f1a493660bba4b9788c5418e4df5f060 of
#: pymc_marketing.bass.model.create_bass_model:23
