#, python-brace-format
msgid ""
"f(\\lambda, p | d, x, t_1, \\dots, t_x, T)\n"
"= f(\\lambda, p | t_x, T) = (1 - p)^{x-1} \\lambda^x \\exp(-\\lambda t_x)\n"
"p^d \\left\\{(1-p)\\exp(-\\lambda*(T - t_x))\\right\\}^{1 - d}"
msgstr ""
"f(\\lambda, p | d, x, t_1, \\dots, t_x, T)  \n"
"= f(\\lambda, p | t_x, T) = (1 - p)^{x-1} \\lambda^x \\exp(-\\lambda t_x)  \n"
"p^d \\left\\{(1-p)\\exp(-\\lambda*(T - t_x))\\right\\}^{1 - d}"

